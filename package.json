{
  "name": "@slack/client",
  "version": "4.0.0-beta.1",
  "description": "Slack Developer Kit - official clients for the Web API, RTM API, and Incoming Webhooks",
  "author": "Slack Technologies, Inc.",
  "license": "MIT",
  "keywords": [
    "slack",
    "web-api",
    "rtm-api",
    "bot",
    "chatbot",
    "client",
    "http",
    "websockets",
    "api"
  ],
  "main": "./dist/index.js",
  "engines": {
    "node": ">= 6.9.0",
    "npm": ">= 3.10.8"
  },
  "repository": "slackapi/node-slack-sdk",
  "bugs": {
    "url": "https://github.com/slackapi/node-slack-sdk/issues"
  },
  "scripts": {
    "prepublish": "npm run build",
    "precommit": "lint-staged",
    "prepush": "npm run build",
    "lint": "tslint --project .",
    "test": "npm run test:spec && nyc mocha",
    "test:spec": "nyc mocha --opts src/mocha.opts src/*.spec.js src/**/*.spec.js",
    "coverage": "codecov",
    "build": "npm run build:clean && tsc",
    "build:clean": "shx rm -rf ./dist ./coverage ./.nyc_output",
    "make-docs": "node jsdoc.js"
  },
  "dependencies": {
    "@types/loglevel": "^1.5.3",
    "@types/p-queue": "^2.3.1",
    "@types/retry": "^0.10.2",
    "async": "^1.5.0",
    "eventemitter3": "^3.0.0",
    "https-proxy-agent": "^1.0.0",
    "lodash": "^4.13.1",
    "loglevel": "^1.6.1",
    "p-queue": "^2.3.0",
    "pkginfo": "^0.4.0",
    "request": ">=2.0.0 <2.77.0",
    "retry": "^0.10.1",
    "url-join": "0.0.1",
    "winston": "^2.1.1",
    "ws": "^1.0.1"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/mocha": "^2.2.48",
    "@types/node": "^9.4.0",
    "busboy": "^0.2.14",
    "capture-stdout": "^1.0.0",
    "chai": "^4.1.2",
    "codecov": "^3.0.0",
    "husky": "^0.14.3",
    "lint-staged": "^6.1.0",
    "mocha": "^5.0.0",
    "nock": "^9.1.6",
    "nyc": "^11.4.1",
    "shx": "^0.2.2",
    "sinon": "^4.2.2",
    "source-map-support": "^0.5.3",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "tslint-config-airbnb": "^5.5.0",
    "typescript": "^2.7.1"
  }
}
